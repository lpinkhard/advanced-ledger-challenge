{
  "version": 2,
  "framework": null,
  "builds": [
    {
      "src": "api/**/*.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "status": 404,
      "dest": "/api/health"
    }
  ],
  "crons": [
    {
      "path": "/api/outbox/process",
      "schedule": "0 0 * * *"
    }
  ]
}
