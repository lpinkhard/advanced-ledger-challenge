{
  "version": 2,
  "rewrites": [
    { "source": "/journal", "destination": "/api/journal" },
    { "source": "/health", "destination": "/api/health" },
    { "source": "/events", "destination": "/api/events" },
    { "source": "/outbox/process", "destination": "/api/outbox/process" },
    { "source": "/accounts/:id/history", "destination": "/api/accounts/history?id=:id" }
  ],
  "crons": [
    { "path": "/outbox/process", "schedule": "0 0 * * *" }
  ]
}
